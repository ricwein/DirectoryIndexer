<meta name="mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="application-name" content="{{ name }}">
<meta name="apple-mobile-web-app-title" content="{{ name }}">
<meta name="google" value="notranslate">
<meta name="DC.Title" content="{{ name }}">
<meta name="DC.Description" content="{{ name }}">
<meta property="og:title" content="{{ name }}">
<meta property="og:description" content="{{ name }}">
<meta property="og:site_name" content="{{ name }}">
<meta property="og:type" content="website">
<meta property="og:url" content="{{ url.base }}">
<meta name="twitter:title" content="{{ name }}">
<meta name="twitter:url" content="{{ url.base }}">
<meta itemprop="name" content="{{ url.base }}">
<meta itemprop="description" content="{{ name }}">
